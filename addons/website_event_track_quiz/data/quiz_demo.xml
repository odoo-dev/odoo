<?xml version="1.0" encoding="utf-8"?>
<odoo><data>

    <record id="event_7_track_1_quiz" model="event.quiz">
        <field name="name">What This Event Is All About</field>
    </record>

    <record id="event_7_track_1_question_0" model="event.quiz.question">
        <field name="name">What does a home absolutely need ?</field>
        <field name="sequence">1</field>
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_1_quiz"/>
    </record>
    <record id="event_7_track_1_question_0_0" model="event.quiz.answer">
        <field name="text_value">Resources</field>
        <field name="sequence">1</field>
        <field name="is_correct" eval="True"/>
        <field name="comment">Correct ! It needs water, electricity and more !</field>
        <field name="question_id" ref="event_7_track_1_question_0"/>
    </record>
    <record id="event_7_track_1_question_0_1" model="event.quiz.answer">
        <field name="text_value">A dog</field>
        <field name="sequence">2</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect ! Even if we all know that dogs are awesome, a home needs resources.</field>
        <field name="question_id" ref="event_7_track_1_question_0"/>
    </record>
    <record id="event_7_track_1_question_0_2" model="event.quiz.answer">
        <field name="text_value">A garage</field>
        <field name="sequence">3</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect ! A garage is just a nice to have. A home needs resources</field>
        <field name="question_id" ref="event_7_track_1_question_0"/>
    </record>
    <record id="event_7_track_1_question_1" model="event.quiz.question">
        <field name="name">Can the earth sustain our current way of living ?</field>
        <field name="sequence">2</field>
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_1_quiz"/>
    </record>
    <record id="event_7_track_1_question_1_0" model="event.quiz.answer">
        <field name="text_value">No !</field>
        <field name="sequence">1</field>
        <field name="is_correct" eval="True"/>
        <field name="comment">Correct ! To prevent global warming, we need to produce cleaner energy which will drastically reduce our carbon emissions !</field>
        <field name="question_id" ref="event_7_track_1_question_1"/>
    </record>
    <record id="event_7_track_1_question_1_1" model="event.quiz.answer">
        <field name="text_value">Yes !</field>
        <field name="sequence">2</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect ! Carbon emissions cause global warming, which is the biggest threat we have ever faced !</field>
        <field name="question_id" ref="event_7_track_1_question_1"/>
    </record>

    <record id="website_event_track.event_7_track_1" model="event.track">
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_1_quiz"/>
    </record>

    <record id="event_7_track_5_quiz" model="event.quiz">
        <field name="name">Securing your furniture – what you need to know</field>
    </record>

    <record id="event_7_track_5_question_0" model="event.quiz.question">
        <field name="name">Are Ikea furniture safe when they are not fixed to a wall ?</field>
        <field name="sequence">1</field>
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_5_quiz"/>
    </record>
    <record id="event_7_track_5_question_0_0" model="event.quiz.answer">
        <field name="text_value">Yes</field>
        <field name="sequence">1</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect! In order to avoid accident, Ikea furniture needs to be secured by fixing them to the wall!</field>
        <field name="question_id" ref="event_7_track_5_question_0"/>
    </record>
    <record id="event_7_track_5_question_0_1" model="event.quiz.answer">
        <field name="text_value">No</field>
        <field name="sequence">2</field>
        <field name="is_correct" eval="True"/>
        <field name="comment">Correct! Even if some Ikea furnitures are self supporting, some others need to be secured.</field>
        <field name="question_id" ref="event_7_track_5_question_0"/>
    </record>
    <record id="event_7_track_5_question_1" model="event.quiz.question">
        <field name="name">What kind of tool are needed to secure your Ikea furniture ?</field>
        <field name="sequence">2</field>
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_5_quiz"/>
    </record>
    <record id="event_7_track_5_question_1_0" model="event.quiz.answer">
        <field name="text_value">Hammer and sickle</field>
        <field name="sequence">1</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect! The sickle won't be of any help here, unless you want some decoration !</field>
        <field name="question_id" ref="event_7_track_5_question_1"/>
    </record>
    <record id="event_7_track_5_question_1_1" model="event.quiz.answer">
        <field name="text_value">Hammer, drill and screwdriver</field>
        <field name="sequence">2</field>
        <field name="is_correct" eval="True"/>
        <field name="comment">Correct!</field>
        <field name="question_id" ref="event_7_track_5_question_1"/>
    </record>
        <record id="event_7_track_5_question_1_2" model="event.quiz.answer">
        <field name="text_value">Scotch tape and timber</field>
        <field name="sequence">3</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect! Well, it could work but it will be kind of overkill, don't you think ?</field>
        <field name="question_id" ref="event_7_track_5_question_1"/>
    </record>

    <record id="website_event_track.event_7_track_5" model="event.track">
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_5_quiz"/>
    </record>

    <record id="event_7_track_13_quiz" model="event.quiz">
        <field name="name">Pretty. Ugly. Lovely.</field>
    </record>

    <record id="event_7_track_13_question_0" model="event.quiz.question">
        <field name="name">What's the name of the Ikea's Creative Leader that was presented here?</field>
        <field name="sequence">1</field>
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_13_quiz"/>
    </record>
    <record id="event_7_track_13_question_0_0" model="event.quiz.answer">
        <field name="text_value">Nike Karlsson</field>
        <field name="sequence">1</field>
        <field name="is_correct" eval="True"/>
        <field name="comment">Correct!</field>
        <field name="question_id" ref="event_7_track_13_question_0"/>
    </record>
    <record id="event_7_track_13_question_0_1" model="event.quiz.answer">
        <field name="text_value">Nicky Larson</field>
        <field name="sequence">2</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect! Nicky Larson is a Private Detective and Body Guard from Tsukasa Hōjō's City Hunter's French TV show adaptation.</field>
        <field name="question_id" ref="event_7_track_13_question_0"/>
    </record>
    <record id="event_7_track_13_question_0_2" model="event.quiz.answer">
        <field name="text_value">Martin Aston</field>
        <field name="sequence">3</field>
        <field name="is_correct" eval="False"/>
        <field name="comment">Incorrect! An Aston Martin is a car notably driven by James Bond and Rowan Atkinson.</field>
        <field name="question_id" ref="event_7_track_13_question_0"/>
    </record>

    <record id="website_event_track.event_7_track_13" model="event.track">
        <field name="quiz_id" ref="website_event_track_quiz.event_7_track_13_quiz"/>
    </record>

</data></odoo>
