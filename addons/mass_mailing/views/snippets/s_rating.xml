<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_rating" name="Rating">
    <div class="s_rating pt16 pb16 o_mail_snippet_general" data-vcss="001" data-icon="fa-star" data-snippet="s_rating">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <h4 class="s_rating_title o_default_snippet_text">Quality</h4>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="s_rating_icons o_not_editable" contenteditable="false">
                        <span class="s_rating_active_icons">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </span>
                        <span class="s_rating_inactive_icons">
                            <i class="fa fa-star-o"></i>
                            <i class="fa fa-star-o"></i>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</template>

</odoo>
