<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.PercentageField" owl="1">
        <t t-if="props.readonly">
            <PercentageViewer
                value="props.value"
            />
        </t>
        <t t-else="">
            <PercentageEditor
                fieldClass="'o_percentage_field'"
                onChange="(ev) => onChange(ev)"
                value="props.value"
            />
        </t>
    </t>

</templates>
