<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.DateField" owl="1">
        <t t-if="props.readonly">
            <span t-esc="formattedValue" />
        </t>
        <t t-else="">
            <DatePicker date="props.value" t-on-datetime-changed="onChange" />
        </t>
    </t>

</templates>
