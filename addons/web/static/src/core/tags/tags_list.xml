<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.TagsList" owl="1">
        <span t-att-class="props.className" t-att-name="props.name">
            <t t-if="props.tags" t-foreach="visibleTags" t-as="tag" t-key="tag.id or tag_index">
                <TagItem t-props="tag" />
            </t>
            <span t-if="props.tags and otherTags" class="o_m2m_avatar_empty rounded-circle text-center font-weight-bold" t-att-data-tooltip="tooltip">
                <span t-esc="'+' + otherTags.length"/>
            </span>
            <t t-slot="default" />
        </span>
    </t>

</templates>
