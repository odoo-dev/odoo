<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="mail.SelectablePartnersList" owl="1">
        <div class="o_SelectablePartnersList">
            <t t-if="invitePartnerList.selectablePartners.length > 0">
                <t t-foreach="invitePartnerList.selectablePartners" t-as="selectabledPartner" t-key="selectabledPartner.localId">
                    <SelectablePartner
                        invitePartnerListLocalId="invitePartnerList.localId"
                        partnerLocalId="selectabledPartner.localId"
                    />
                </t>
            </t>
            <t t-if="invitePartnerList.selectablePartners.length === 0">
                <div class="o_SelectablePartnersList_noResult">No partner found that is not already a member of this group.</div>
            </t>
        </div>
    </t>

</templates>
